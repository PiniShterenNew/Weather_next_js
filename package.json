{
  "name": "wheather-next-js",
  "version": "2.0.3",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "npm run build:prisma && npm run set-version && npm run build:next",
    "build:prisma": "prisma generate",
    "build:next": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:fast": "SKIP_ENV_VALIDATION=1 next build",
    "set-version": "echo NEXT_PUBLIC_APP_VERSION=$npm_package_version > .env.local",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "lint:cache": "eslint . --ext .js,.jsx,.ts,.tsx --cache --cache-location .eslintcache",
    "format": "prettier --write .",
    "clean": "npm run lint:fix && npm run format",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run tests/unit",
    "test:unit:watch": "vitest tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:integration:watch": "vitest tests/integration",
    "test:unit:coverage": "vitest run --coverage tests/unit",
    "test:integration:coverage": "vitest run --coverage tests/integration",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:seed": "prisma db seed",
    "prisma:reset": "prisma db reset --force",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:report": "playwright show-report",
    "e2e:chrome": "playwright test --project=chromium",
    "e2e:chrome:single": "playwright test --project=chromium --workers=1",
    "e2e:fast": "playwright test --project=chromium --workers=1 e2e/city-search.spec.ts e2e/language-switch.spec.ts e2e/unit-switch.spec.ts e2e/onboarding-flow.spec.ts e2e/manual-refresh.spec.ts e2e/offline-online.spec.ts",
    "e2e:essential": "playwright test --project=chromium --workers=1 e2e/city-search.spec.ts e2e/language-switch.spec.ts e2e/unit-switch.spec.ts",
    "e2e:firefox": "playwright test --project=firefox --workers=1",
    "e2e:webkit": "playwright test --project=webkit --workers=1",
    "lighthouse": "lhci autorun"
  },
  "dependencies": {
    "@clerk/localizations": "^3.25.7",
    "@clerk/nextjs": "^6.33.3",
    "@clerk/themes": "^2.4.25",
    "@prisma/client": "^6.11.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-primitive": "^2.1.3",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tanstack/react-virtual": "^3.13.12",
    "@use-gesture/react": "^10.3.1",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "framer-motion": "^12.23.0",
    "lucide-react": "^0.514.0",
    "next": "^15.3.3",
    "next-intl": "^4.1.0",
    "postcss": "^8.5.5",
    "prisma": "^6.11.1",
    "rate-limiter-flexible": "^7.1.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "web-push": "^3.6.7",
    "workbox-build": "^7.1.1",
    "workbox-core": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@ducanh2912/next-pwa": "^10.2.9",
    "@lhci/cli": "^0.15.1",
    "@next/eslint-plugin-next": "^15.5.4",
    "@playwright/test": "^1.53.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.19.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/web-push": "^3.6.3",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "critters": "^0.0.20",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unicorn": "^59.0.1",
    "jsdom": "^26.1.0",
    "lighthouse": "^12.7.1",
    "null-loader": "^4.0.1",
    "prettier": "^3.5.3",
    "rollup": "^4.52.4",
    "tsx": "^4.20.5",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "browserslist": {
    "production": [
      "last 3 Chrome versions",
      "last 3 Firefox versions",
      "last 3 Edge versions",
      "last 3 Safari versions",
      "not IE 11"
    ],
    "development": [
      "last 1 Chrome version",
      "last 1 Firefox version",
      "last 1 Safari version"
    ]
  }
}
